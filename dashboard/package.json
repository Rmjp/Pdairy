{
	"name": "dashboard",
	"version": "0.0.1",
	"scripts": {
		"prepare": "npm run smui-theme",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview --host --port 3000",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@smui/button": "^7.0.0-beta.14",
		"@smui/card": "^7.0.0-beta.14",
		"@smui/data-table": "^7.0.0-beta.14",
		"@smui/form-field": "^7.0.0-beta.14",
		"@smui/textfield": "^7.0.0-beta.14",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.5.0",
		"@types/cookie": "^0.5.1",
		"autoprefixer": "^10.4.14",
		"postcss": "^8.4.25",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"smui-theme": "^7.0.0-beta.14",
		"svelte": "^3.54.0",
		"svelte-check": "^3.0.1",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@sveltejs/adapter-node": "^1.3.0"
	}
}
